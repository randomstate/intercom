{
    "name": "contingens/intercom",
    "description": "Intercom integration package for PHP, with great support for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Connor Forsyth",
            "email": "connor.forsyth@contingens.com"
        }
    ],
    "require": {
        "intercom/intercom-php": "^4.0"
    },
    "autoload": {
      "psr-4": {
        "Contingens\\Intercom\\" : "src"
      }
    },
    "autoload-dev": {
      "classmap": [
        "vendor/laravel/laravel/tests/TestCase.php"
      ],
      "psr-4": {
        "Contingens\\Intercom\\Tests\\" : "tests",
        "Tests\\": "vendor/laravel/laravel/tests"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "laravel/laravel": "^5.7",
        "mockery/mockery": "^1.2"
    }
}
